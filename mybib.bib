	file = {Gossel et al. - 2018 - Coupled-Trajectory Mixed Quantum-Classical Algorit.pdf:/home/Sangeya/Zotero/storage/AZ57BUIW/Gossel et al. - 2018 - Coupled-Trajectory Mixed Quantum-Classical Algorit.pdf:application/pdf}
