@article{gossel_coupled-trajectory_2018,
	title = {Coupled-{Trajectory} {Mixed} {Quantum}-{Classical} {Algorithm}: {A} {Deconstruction}},
	issn = {1549-9618, 1549-9626},
	shorttitle = {Coupled-{Trajectory} {Mixed} {Quantum}-{Classical} {Algorithm}},
	url = {http://pubs.acs.org/doi/10.1021/acs.jctc.8b00449},
	doi = {10.1021/acs.jctc.8b00449},
	abstract = {We analyze a mixed quantum-classical algorithm recently derived from the exact factorization equations [Min, Agostini, Gross, PRL 115, 073001 (2015)] to show the role of the diﬀerent terms in the algorithm in bringing about decoherence and wavepacket branching. The algorithm has the structure of Ehrenfest equations plus a “coupled-trajectory” term for both the electronic and nuclear equations, and we analyze the relative roles played by the diﬀerent nonadiabatic terms in these equations, including how they are computed in practice. In particular, we show that while the coupled-trajectory term in the electronic equation is essential in yielding accurate dynamics, that in the nuclear equation has a much smaller eﬀect. A decoherence time is extracted from the electronic equations and compared with that of augmented fewest-switches surfacehopping. We revisit a series of nonadiabatic Tully model systems to illustrate our analysis.},
	language = {en},
	urldate = {2018-08-28},
	journal = {Journal of Chemical Theory and Computation},
	author = {Gossel, Graeme H. and Agostini, Federica and Maitra, Neepa T.},
